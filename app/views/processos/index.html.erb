

<h1>Processos</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Numero</th>
      <th>Assunto</th>
      <th>Descricao</th>
      <th>Situacao</th>
      <th>Local</th>
      <th>Contato</th>
      <th>Criado por</th>
      <th>Atualizado por</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processos.each do |processo| %>
      <tr>
        <td><%= processo.data %></td>
        <td><%= processo.numero %></td>
        <td><%= processo.assunto %></td>
        <td><%= processo.descricao %></td>
        <td><%= processo.situacao %></td>
        <td><%= processo.local %></td>
        <td><%= processo.contato %></td>
        <td><%= processo.criado_por %></td>
        <td><%= processo.atualizado_por %></td>
        <td><%= processo.tipo %></td>
        <td><%= link_to 'Show', processo %></td>
        <td><%= link_to 'Edit', edit_processo_path(processo) %></td>
        <td><%= link_to 'Destroy', processo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Processo', new_processo_path %>
