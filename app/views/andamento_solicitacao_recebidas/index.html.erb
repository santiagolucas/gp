

<h1>Andamento Solicitacao Recebidas</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Descricao</th>
      <th>Observacao</th>
      <th>Funcionario</th>
      <th>Solicitacao recebida</th>
      <th>Criado por</th>
      <th>Atualizado por</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @andamento_solicitacao_recebidas.each do |andamento_solicitacao_recebida| %>
      <tr>
        <td><%= andamento_solicitacao_recebida.data %></td>
        <td><%= andamento_solicitacao_recebida.descricao %></td>
        <td><%= andamento_solicitacao_recebida.observacao %></td>
        <td><%= andamento_solicitacao_recebida.funcionario_id %></td>
        <td><%= andamento_solicitacao_recebida.solicitacao_recebida_id %></td>
        <td><%= andamento_solicitacao_recebida.criado_por %></td>
        <td><%= andamento_solicitacao_recebida.atualizado_por %></td>
        <td><%= link_to 'Show', andamento_solicitacao_recebida %></td>
        <td><%= link_to 'Edit', edit_andamento_solicitacao_recebida_path(andamento_solicitacao_recebida) %></td>
        <td><%= link_to 'Destroy', andamento_solicitacao_recebida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

